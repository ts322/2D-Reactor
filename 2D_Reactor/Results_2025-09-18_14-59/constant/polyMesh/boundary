(
    inlet
    {
        type            patch;
        nFaces          20;
        startFace       7780;
    }
    outlet
    {
        type            patch;
        nFaces          20;
        startFace       7800;
    }
    walls
    {
        type            wall;
        inGroups        1(wall);
        nFaces          400;
        startFace       7820;
    }
    frontAndBack
    {
        type            empty;
